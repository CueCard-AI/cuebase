# Environment variables for Autobase testing
# Copy this file to .env and fill in your values
# Then source it with: source .env

# AWS credentials for S3 backup testing
export AWS_ACCESS_KEY_ID="your_access_key_id"
export AWS_SECRET_ACCESS_KEY="your_secret_access_key"
export AWS_DEFAULT_REGION="us-east-2"

# PostgreSQL connection details
export PGPASSWORD="your_postgres_password"

# Cloudflare credentials for Cloudflare integration testing
export CLOUDFLARE_API_TOKEN="your_cloudflare_api_token"
export CLOUDFLARE_ZONE_ID="your_cloudflare_zone_id"
export CLOUDFLARE_ACCOUNT_ID="your_cloudflare_account_id" 